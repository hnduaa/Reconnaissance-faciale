    {
        "id": "03ec1f5b86cff498",
        "type": "function",
        "z": "ad0f97f190e925de",
        "name": "Pr√©parer email alerte",
        "func": "// --- INFOS DE L'ALERTE ---\nconst identity = msg.payload.identity || \"Inconnu\";\nconst timestamp = new Date().toLocaleString();\n\n// Variables pour traiter l'image\nlet imageAttachment = null;\n\n// --- V√âRIFICATION DE L'IMAGE ---\n// V√©rifier o√π se trouve l'image dans le message\nif (msg.payload.image) {\n    // Si l'image est directement dans msg.payload.image\n    imageAttachment = msg.payload.image;\n} else if (msg.image) {\n    // Si l'image est dans msg.image\n    imageAttachment = msg.image;\n} else if (typeof msg.payload === 'object' && msg.payload.hasOwnProperty('payload') && msg.payload.payload.image) {\n    // Si l'image est dans msg.payload.payload.image (message imbriqu√©)\n    imageAttachment = msg.payload.payload.image;\n}\n\n// --- CONSTRUCTION DU HTML ---\nmsg.topic = \"‚ö†Ô∏è ALERTE S√âCURIT√â - Acc√®s non autoris√©\";\nmsg.payload = `\n<!DOCTYPE html>\n<html lang=\"fr\">\n<head><meta charset=\"UTF-8\">\n  <style>\n    body { font-family: Arial, sans-serif; background: #f4f4f9; padding: 20px; color: #333; }\n    h2 { color: #e74c3c; }\n    .alert { background: #ffcccc; padding: 10px; border-radius: 5px; }\n    .footer { font-size: 12px; color: #888; margin-top: 20px; }\n  </style>\n</head>\n<body>\n  <h2>Alerte de s√©curit√©</h2>\n  <div class=\"alert\">\n    <p><strong>Une personne non autoris√©e a √©t√© d√©tect√©e.</strong></p>\n    <p><strong>Date/heure :</strong> ${timestamp}</p>\n    <p><strong>Identit√© :</strong> ${identity}</p>\n  </div>\n  <p>Une image est jointe √† cet email.</p>\n  <p class=\"footer\"><em>Cet email a √©t√© g√©n√©r√© automatiquement par le syst√®me de s√©curit√©.</em></p>\n</body>\n</html>\n`;\n\n// --- PR√âPARATION DE LA PI√àCE JOINTE ---\nif (imageAttachment) {\n    try {\n        // V√©rifier si l'image est d√©j√† un Buffer ou une cha√Æne Base64\n        let imageBuffer;\n        if (Buffer.isBuffer(imageAttachment)) {\n            imageBuffer = imageAttachment;\n        } else {\n            // Essayer de convertir la cha√Æne Base64 en Buffer\n            imageBuffer = Buffer.from(imageAttachment, \"base64\");\n        }\n\n        // G√©n√©rer un nom de fichier coh√©rent\n        const safeTs = timestamp.replace(/[\\/:\\s,]/g, \"_\");\n        const filename = `alerte_${safeTs}.jpg`;\n\n        // Ajouter la pi√®ce jointe\n        msg.attachments = [{\n            filename: filename,\n            content: imageBuffer,\n            contentType: \"image/jpeg\"\n        }];\n\n        // Ajouter un message de d√©bogage\n        node.status({ fill: \"green\", shape: \"dot\", text: \"Image jointe ajout√©e\" });\n    } catch (error) {\n        // En cas d'erreur, ajouter un message dans les logs\n        node.error(\"Erreur lors du traitement de l'image: \" + error.message, msg);\n        node.status({ fill: \"red\", shape: \"ring\", text: \"Erreur avec l'image\" });\n    }\n} else {\n    // Pas d'image trouv√©e\n    node.status({ fill: \"yellow\", shape: \"ring\", text: \"Aucune image trouv√©e\" });\n}\n\n// Retourner le message vers le n≈ìud Email\nreturn msg;",
