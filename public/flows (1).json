    {
        "id": "a2bbbefda2f1e91e",
        "type": "function",
        "z": "ad0f97f190e925de",
        "name": "Formater pour historique",
        "func": "// 1. Lire l'historique actuel depuis le flow context\nlet history = flow.get(\"history\") || [];\n\n// 2. Pr√©parer la nouvelle entr√©e\nconst timestamp = new Date().toLocaleString();\nconst status    = msg.payload.status;\nconst identity  = msg.payload.identity || \"Inconnu\";\nconst image     = msg.payload.image;\nconst entry     = {\n  date:     timestamp,\n  identity: identity,\n  status:   status,\n  image:    image\n    ? `<img src=\"data:image/jpeg;base64,${image}\" width=\"50\">`\n    : \"Pas d'image\"\n};\n\n// 3. Ajouter la nouvelle entr√©e √† l'historique\nhistory.push(entry);\n\n// (Optionnel) Limiter la taille de l‚Äôhistorique √†, par exemple, 100 lignes\n// if (history.length > 100) { history.shift(); }\n\n// 4. Sauvegarder l'historique mis √† jour dans le flow context\nflow.set(\"history\", history);\n\n// 5. Pr√©parer les sorties\n// Sortie 1 : tout l'historique pour affichage dans le dashboard\nlet out1 = { payload: history };\n\n// Sortie 2 : derni√®re entr√©e (entry) pour envoi MQTT\nlet out2 = { payload: entry };\n\n// Sortie 3 : valeur num√©rique pour le Gauge (0 = untrusted, 1 = trusted)\nconst numeric = (status === \"trusted\" ? 1 : 0);\nlet out3 = { payload: numeric };\n\n// 6. Retourner un tableau de 3 messages\nreturn [ out1, out2, out3 ];\n",
